{
  "name": "internal-chat",
  "version": "1.1.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build": "pkg . --out-path dist",
    "build:all": "pkg . --targets node16-linux-x64,node16-linux-arm64,node16-win-x64,node16-macos-x64,node16-macos-arm64 --out-path dist",
    "build:linux": "pkg . --targets node16-linux-x64 --out-path dist",
    "build:linux-arm": "pkg . --targets node16-linux-arm64 --out-path dist",
    "build:windows": "pkg . --targets node16-win-x64 --out-path dist", 
    "build:macos": "pkg . --targets node16-macos-x64 --out-path dist",
    "build:macos-arm": "pkg . --targets node16-macos-arm64 --out-path dist"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "bin": "./index.js",
  "pkg": {
    "assets": [
      "!room_pwd.json",
      "./node_modules/**/*",
      "./www/**/*"
    ],
    "ignore": [
      "./room_pwd.json"
    ],
    "targets": [
      "node16-linux-x64",
      "node16-linux-arm64",
      "node16-win-x64",
      "node16-macos-x64",
      "node16-macos-arm64"
    ],
    "outputPath": "dist"
  }
}
